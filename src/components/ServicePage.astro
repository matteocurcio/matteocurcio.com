---
interface Props {
  title: string;
  intro: string;
  list: string[];
}

const { title, intro, list } = Astro.props;
---

<section>
  <h1>{title}</h1>
  <p>{intro}</p>
</section>

<section class="section">
  <h2>What You Can Expect</h2>
  <div class="projects-grid">
    {list.map((item) => (
      <article class="card">
        <div class="card-body">
          <h3>{item}</h3>
        </div>
      </article>
    ))}
  </div>
</section>
